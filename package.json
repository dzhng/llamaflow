    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/jest": "^29.5.0",
    "@types/jsonic": "^0.3.1",
    "@types/lodash": "^4.14.191",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-universe": "^11.1.1",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2"
